{
  "name": "othent-vue-components",
  "version": "0.0.1",
  "description": "A Othent.io Vue Components Library",
  "main": "./dist/othent-vue-components.umd.js",
  "module": "./dist/othent-vue-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/othent-vue-components.es.js",
      "require": "./dist/othent-vue-components.umd.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "release": "release-it"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "othent"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawanpaudel93/othent-vue-components.git"
  },
  "author": "Pawan Paudel",
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "othent": "^1.0.616"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "release-it": "^15.10.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.2"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {}
          ]
        }
      }
    }
  }
}
