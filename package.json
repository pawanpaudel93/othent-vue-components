{
  "name": "othent-vue-components",
  "version": "0.0.9",
  "type": "module",
  "description": "A Othent.io Vue Components Library",
  "main": "./dist/othent-vue-components.umd.cjs",
  "module": "./dist/othent-vue-components.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/othent-vue-components.js",
      "require": "./dist/othent-vue-components.umd.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --config vite.dev.config.ts",
    "package": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "release": "release-it"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "othent"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawanpaudel93/othent-vue-components.git"
  },
  "author": "Pawan Paudel",
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "othent": "^1.0.619"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.14.1",
    "prettier": "^2.8.8",
    "prettier-plugin-vue": "^1.1.6",
    "release-it": "^15.10.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-css-injected-by-js": "^3.1.1",
    "vite-plugin-dts": "^2.3.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.2"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {}
          ]
        }
      }
    }
  }
}
